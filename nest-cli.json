{
  "collection": "@nestjs/schematics",
  "sourceRoot": "./",
  "monorepo": true,
  "root": "./",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/profe/tsconfig.app.json",
    "assets": [
      {
        "include": "**/*.hbs",
        "watchAssets": true
      }
    ]
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "calendar": {
      "type": "application",
      "root": "apps/calendar",
      "entryFile": "main",
      "sourceRoot": "apps/calendar/src",
      "compilerOptions": {
        "tsConfigPath": "apps/calendar/tsconfig.app.json"
      }
    },
    "teacher": {
      "type": "application",
      "root": "apps/teacher",
      "entryFile": "main",
      "sourceRoot": "apps/teacher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/teacher/tsconfig.app.json"
      }
    },
    "timeline": {
      "type": "application",
      "root": "apps/timeline",
      "entryFile": "main",
      "sourceRoot": "apps/timeline/src",
      "compilerOptions": {
        "tsConfigPath": "apps/timeline/tsconfig.app.json"
      }
    },
    "categories": {
      "type": "application",
      "root": "apps/categories",
      "entryFile": "main",
      "sourceRoot": "apps/categories/src",
      "compilerOptions": {
        "tsConfigPath": "apps/categories/tsconfig.app.json"
      }
    },
    "category": {
      "type": "application",
      "root": "apps/category",
      "entryFile": "main",
      "sourceRoot": "apps/category/src",
      "compilerOptions": {
        "tsConfigPath": "apps/category/tsconfig.app.json"
      }
    }
  }
}