{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/profe/src",
  "monorepo": true,
  "root": "apps/profe",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/profe/tsconfig.app.json"
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "categories": {
      "type": "application",
      "root": "apps/categories",
      "entryFile": "main",
      "sourceRoot": "apps/categories/src",
      "compilerOptions": {
        "tsConfigPath": "apps/categories/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "teacher": {
      "type": "application",
      "root": "apps/teacher",
      "entryFile": "main",
      "sourceRoot": "apps/teacher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/teacher/tsconfig.app.json"
      }
    }
  }
}