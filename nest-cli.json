{
  "collection": "@nestjs/schematics",
  "sourceRoot": "./",
  "monorepo": true,
  "root": "./",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/profe/tsconfig.app.json",
    "assets": [
      {
        "include": "**/*.hbs",
        "watchAssets": true
      }
    ]
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
